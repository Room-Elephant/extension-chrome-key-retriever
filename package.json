  "repository": {
    "type": "git",
    "url": "git+https://github.com/Room-Elephant/extension-chrome-key-retriever.git"
  },
  "keywords": [
    "chrome-extension",
    "localstorage",
    "sessionstorage",
    "cookiestorage"
  ],
  "author": "Room-Elephant",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Room-Elephant/extension-chrome-key-retriever/issues"
  },
  "homepage": "https://github.com/Room-Elephant/extension-chrome-key-retriever#readme",
  "devDependencies": {
    "prettier": "^3.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "copy-webpack-plugin": "^11.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "html-minimizer-webpack-plugin": "^4.4.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "uglify-js": "^3.17.4"
